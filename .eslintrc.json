{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "env": { "es6": true },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true,
      "jsx": true
    }
  },
  "rules": {
    "no-unused-vars": ["error", { "args": "none" }]
  },
  "globals": {
    "log": false,
    "global": false
  }
}